fun void test(...) {
  int i;
  vararg.start;

	if(i == 0)
		<<< vararg $ int >>>;
	else if(i == 1)
		<<< vararg $ float >>>;
	else if(i == 2)
		<<< vararg $ complex >>>;
	else if(i == 3)
		<<< vararg $ polar >>>;
	else if(i == 4)
		<<< vararg $ Object >>>;
  	i++;
  vararg.end;
}
test(1);
test(1, 2.3, complex(3, 2), polar(3, 2.1), null);
